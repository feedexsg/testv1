<div id="newitem">

<%= link_to "< Back", admin_items_path, :class => "backbutton" %>

<h1>Add New</h1> <hr />

<div id="newitembox">

<%= form_for [:admin, @item], :html => {:id => 'newitemform'}, multipart: true do |f| %>
	<%= render :partial => "form", locals: { item: @item, f: f } %>
	<div>
		<%= f.submit "Add New", :class => "submitnewitem" %>
	</div>
<% end %>

</div><!--end of newitembox-->

</div><!--end of newitem-->	


